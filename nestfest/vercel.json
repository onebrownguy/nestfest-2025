{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production",
    "BUILD_TIMESTAMP": "2025-08-28T00:07:30Z",
    "FORCE_REBUILD": "true"
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=0"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/websocket",
      "destination": "/api/websocket/route"
    }
  ]
}